{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "stop": "npm run build && yarn build && lsof -t -i :5001 -i:5002 -i:8083 -i:5001 -i:5003 -i:9000 -i:5000 -i:8084 | xargs kill -9",
    "localrun": "npm run build && yarn build && lsof -t -i :5001 -i:5002 -i:8083 -i:5001 -i:5003 -i:9000 -i:5000 -i:8084 | xargs kill -9 && firebase emulators:start"
  },
  "main": "lib/index.js",
  "dependencies": {
    "apollo-server-express": "^2.5.0",
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "firebase-functions-helper": "^0.11.0",
    "graphql": "^15.5.0",
    "http-status-codes": "^1.3.2",
    "request": "^2.88.2",
    "graphql-yoga": "^1.13.1"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.2",
    "firebase-functions-test": "^0.2.0",
    "prettier": "1.18.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.12.0",
    "typescript": "^3.8.0"
  },
  "private": true
}
